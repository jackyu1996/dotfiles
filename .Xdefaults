! Font Settings
Xft*antialias:               true
Xft*hinting:                 true
Xft*hintstyle:               hintslight

! URxvt Settings
URxvt*font:                  xft:Monaco:pixelsize=19,xft:WenQuanYi MicroHei Mono:pixelsize=19:minspace=false,xft:Inconsolata Nerd Font Mono:pixelsize=19:minspace=false
URxvt*letterSpace:           0
URxvt*loginShell:            true
URxvt*termName:              screen-256color
URxvt*shading:               10
URxvt*saveLines:             20000
URxvt*depth:                 32
URxvt*scrollBar:             false
URxvt*cursorBlink:           true
URxvt*background:            [90]#1d1f21
URxvt*perl-ext:
URxvt*perl-ext-common:

! Color Settings
*foreground:                 #c5c8c6
*background:                 #1d1f21
*cursorColor:                #c5c8c6
*color0:                     #282a2e
*color8:                     #373b41
*color1:                     #a54242
*color9:                     #cc6666
*color2:                     #8c9440
*color10:                    #b5bd68
*color3:                     #de935f
*color11:                    #f0c674
*color4:                     #5f819d
*color12:                    #81a2be
*color5:                     #85678f
*color13:                    #b294bb
*color6:                     #5e8d87
*color14:                    #8abeb7
*color7:                     #707880
*color15:                    #c5c8c6

!! Rofi Configs
rofi.font:                   Monaco 12
rofi.bw:                     0
rofi.fixed-num-lines:        false
rofi.show-icons:             true
rofi.terminal:               termite
rofi.sort:                   true
rofi.levenshtein-sort:       true
rofi.combi-modi:             window,run
rofi.fake-transparency:      true
rofi.fake-background:        background
rofi.window-format:          {t}
rofi.theme:                  android_notification
rofi.combi-hide-mode-prefix: true
